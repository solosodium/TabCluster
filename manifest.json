{
  "manifest_version": 2,

  "name": "Tab Cluster",
  "description": "Extension to cluster tabs with the same root URL.",
  "version": "1.0",

  "permissions": [
    "tabs"
  ],

  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "list.html"
  },

  "background":{
    "scripts": ["background.js"]
  },

  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
